@model IEnumerable<ExtensionUserProfile.Models.ZUserProfile>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<div class="panel panel-primary">
    <div class="panel-heading">From IdentityUser</div>
    <ul class="list-group">
        <li class="list-group-item"><mark>Id:</mark> @Model.First().ZUser.Id</li>
        <li class="list-group-item"><mark>LockoutEnabled:</mark> @Model.First().ZUser.LockoutEnabled</li>
        <li class="list-group-item"><mark>PasswordHash:</mark> @Model.First().ZUser.PasswordHash</li>
        <li class="list-group-item"><mark>UserName:</mark> @Model.First().ZUser.UserName</li>
    </ul>
</div>

<br />
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ZUser.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Item)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ZUser.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Item)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
